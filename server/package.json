{
  "name": "typescript-postre-graphql",
  "version": "0.0.1",
  "description": "Awesome project developed with TypeORM.",
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/connect-redis": "0.0.14",
    "@types/express": "^4.17.7",
    "@types/express-rate-limit": "^5.0.0",
    "@types/ioredis": "^4.17.2",
    "@types/jest": "^26.0.7",
    "@types/node": "^14.0.24",
    "@types/rate-limit-redis": "^1.7.1",
    "@types/uuid": "^8.0.0",
    "jest": "^26.1.0",
    "nodemon": "^2.0.4",
    "ts-jest": "^26.1.3",
    "ts-node": "8.10.2",
    "typescript": "3.9.7"
  },
  "dependencies": {
    "@govau-platforms/notify-client": "^5.2.4",
    "@types/axios": "^0.14.0",
    "@types/express-session": "^1.17.0",
    "@types/node-fetch": "^2.5.7",
    "@types/request-promise": "^4.1.46",
    "@types/yup": "^0.29.6",
    "apollo-server-express": "^2.16.0",
    "axios": "^0.19.2",
    "bcrypt": "^5.0.0",
    "connect-redis": "^5.0.0",
    "copyfiles": "^2.3.0",
    "dotenv": "^8.2.0",
    "express-rate-limit": "^5.1.3",
    "express-session": "^1.17.1",
    "graphql": "^15.3.0",
    "graphql-middleware": "^4.0.2",
    "graphql-request": "^2.1.0-next.1",
    "graphql-tools": "^6.0.15",
    "ioredis": "^4.17.3",
    "node-fetch": "^2.6.0",
    "pg": "^8.3.0",
    "rate-limit-redis": "^2.0.0",
    "reflect-metadata": "^0.1.13",
    "request-promise": "^4.2.6",
    "rimraf": "^3.0.2",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typeorm": "0.2.25",
    "uuid": "^8.2.0",
    "yup": "^0.29.3"
  },
  "scripts": {
    "start": "NODE_ENV=production PORT=5000 node ./dist/index.js",
    "watch": "NODE_ENV=development nodemon --exec ts-node src/index.ts",
    "build": "rimraf ./dist && tsc && copyfiles -u 2 **/*.graphql dist/modules",
    "test-server": "NODE_ENV=test ts-node src/index.ts",
    "test": "NODE_ENV=test jest --force-exit --verbose false --runInBand --detectOpenHandles"
  },
  "jest": {
    "globalSetup": "./setup-tests.js",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
